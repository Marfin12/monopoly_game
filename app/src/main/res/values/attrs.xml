<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GameButton">
        <attr name="text" format="string" />
        <attr name="symbol" format="string" />
        <attr name="symbol_top" format="integer" />
        <attr name="symbol_start" format="integer" />
        <attr name="symbol_end" format="integer" />
        <attr name="symbol_bottom" format="integer" />
    </declare-styleable>

    <declare-styleable name="GameMessage">
        <attr name="title" format="string" />
        <attr name="message" format="string" />
        <attr name="positive_button" format="string" />
        <attr name="negative_button" format="string"/>
    </declare-styleable>
</resources>