<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="120dp"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/iv_left_menu"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:src="@drawable/left_menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tint="#fff" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_profile_new"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/profile"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom_iv_profile"
            app:layout_constraintEnd_toEndOf="@id/guideline_end_iv_profile"
            app:layout_constraintStart_toStartOf="@id/guideline_start_iv_profile"
            app:layout_constraintTop_toTopOf="@id/guideline_top_iv_profile" />


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top_iv_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.02" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom_iv_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.192" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_start_iv_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.200" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_end_iv_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.683" />

        <com.example.experiments2.component.label.GameNormalLabel
            android:id="@+id/iv_holder_name_bg"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:text="Pumi"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom_iv_holder_name_bg"
            app:layout_constraintEnd_toEndOf="@id/guideline_end_iv_holder_name_bg"
            app:layout_constraintStart_toStartOf="@id/guideline_start_iv_holder_name_bg"
            app:layout_constraintTop_toTopOf="@id/guideline_top_iv_holder_name_bg" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top_iv_holder_name_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.16" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom_iv_holder_name_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.216" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_start_iv_holder_name_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.2" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_end_iv_holder_name_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.68" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_holder_money_info"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/money_icon"
            app:layout_constraintEnd_toEndOf="@id/guideline_end_iv_profile"
            app:layout_constraintStart_toStartOf="@id/guideline_start_iv_profile"
            app:layout_constraintTop_toTopOf="@id/guideline_top_money_info"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom_money_info"
            />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top_money_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.245" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom_money_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.441" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top_iv_holder_money_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.390" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom_iv_holder_money_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.444" />

        <com.example.experiments2.component.label.GameNormalLabel
            android:id="@+id/iv_holder_cash_bg"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:text="$ 21234"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom_iv_holder_cash_bg"
            app:layout_constraintEnd_toEndOf="@id/guideline_end_iv_holder_cash_bg"
            app:layout_constraintStart_toStartOf="@id/guideline_start_iv_holder_cash_bg"
            app:layout_constraintTop_toTopOf="@id/guideline_top_iv_holder_cash_bg" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top_iv_holder_cash_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.385" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom_iv_holder_cash_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.441" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_start_iv_holder_cash_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.2" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_end_iv_holder_cash_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.68" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/iv_holder_asset"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/asset_icon"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="@id/guideline_end_iv_profile"
            app:layout_constraintStart_toStartOf="@id/guideline_start_iv_profile"
            app:layout_constraintTop_toTopOf="@id/guideline_top_holder_asset"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom_holder_asset"
            />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top_holder_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.52" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom_holder_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.72" />


        <com.example.experiments2.component.button.GameNormalButton
            android:id="@+id/btn_game_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:text="Back"
            app:symbol="â†©"
            app:layout_constraintStart_toStartOf="@id/guideline_start_btn_game_back"
            app:layout_constraintEnd_toEndOf="@id/guideline_end_btn_game_back"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom_btn_game_back"
            />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_start_btn_game_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.046" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_end_btn_game_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.799" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom_btn_game_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.93" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_player_asset"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="6"
            android:textColor="@color/white"
            app:fontFamily="@font/poppins_light"
            app:autoSizeTextType="uniform"
            app:autoSizeMinTextSize="6sp"
            app:autoSizeMaxTextSize="100sp"
            app:autoSizeStepGranularity="1sp"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom_tv_player_asset"
            app:layout_constraintEnd_toEndOf="@id/guideline_end_tv_player_asset"
            app:layout_constraintStart_toStartOf="@id/guideline_start_tv_player_asset"
            app:layout_constraintTop_toTopOf="@id/guideline_top_tv_player_asset"
            />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top_tv_player_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.622" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom_tv_player_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.670" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_start_tv_player_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.425" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_end_tv_player_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_until_asset"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="6"
            android:textColor="@color/white"
            app:fontFamily="@font/poppins_light"
            app:autoSizeTextType="uniform"
            app:autoSizeMinTextSize="6sp"
            app:autoSizeMaxTextSize="100sp"
            app:autoSizeStepGranularity="1sp"
            app:layout_constraintBottom_toBottomOf="@id/guideline_bottom_tv_until_asset"
            app:layout_constraintEnd_toEndOf="@id/guideline_end_tv_until_asset"
            app:layout_constraintStart_toStartOf="@id/guideline_start_tv_until_asset"
            app:layout_constraintTop_toTopOf="@id/guideline_top_tv_until_asset"
            />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_top_tv_until_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.572" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_bottom_tv_until_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.620" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_start_tv_until_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.375" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline_end_tv_until_asset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.45" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>